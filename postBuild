# Install AWS CLI
# https://docs.aws.amazon.com/cli/latest/userguide/install-cliv2-linux.html
cd /tmp
curl "https://awscli.amazonaws.com/awscli-exe-linux-x86_64-2.2.11.zip" -o "awscliv2.zip"
unzip awscliv2.zip
./aws/install -i /srv/conda/aws-cli -b /srv/conda/bin
rm -r aws awscli-exe-linux-x86_64-2.2.11.zip
